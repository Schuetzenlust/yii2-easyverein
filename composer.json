{
  "name": "schuetzenlust/easyverein",
  "description": "EasyVerein API v1.7 Client for Yii2",
  "type": "yii2-extension",
  "keywords": ["yii2", "extension", "easyverein", "api"],
  "license": "MIT",
  "authors": [
    {
      "name": "Daniel Lucas",
      "email": "daniel.lucas@neusser-schuetzenlust.de"
    }
  ],
  "require": {
    "yiisoft/yii2": "^2.0",
    "guzzlehttp/guzzle": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "schuetzenlust\\easyverein\\": "src/"
    }
  },
  "extra": {
    "yii\\composer\\Installer::postInstall": [
      "yii\\composer\\Installer::postInstall"
    ],
    "yii\\composer\\Installer::postUpdate": [
      "yii\\composer\\Installer::postUpdate"
    ]
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
